{
  "name": "signet-monorepo",
  "private": true,
  "engines": {
    "node": ">=20.19.0"
  },
  "workspaces": [
    "apps/signet",
    "apps/signet-ui",
    "packages/*"
  ],
  "scripts": {
    "prepare": "pnpm --filter @signet/types run build",
    "build": "pnpm --filter signet run build",
    "build:daemon": "pnpm --filter signet run build",
    "build:ui": "pnpm --filter signet-ui run build",
    "start:daemon": "pnpm --filter signet run lfg",
    "start:ui": "pnpm --filter signet-ui run start",
    "dev:ui": "pnpm --filter signet-ui run dev",
    "sync-version": "node scripts/sync-version.mjs"
  },
  "version": "1.2.0",
  "pnpm": {
    "overrides": {
      "http-proxy": "npm:http-proxy-3@^1.23.2"
    }
  }
}
